terminology
    term_definitions = <
        ["en"] = <
            ["id1"] = <
                text = <"DimensionalFeature">
                description = <"Characterizations of the physical size of an anatomical feature.">
            >
            ["id2"] = <
                text = <"area">
                description = <"The extent of a 2-dimensional feature enclosed within a boundary, for example, the area of a wound.">
            >
            ["id3"] = <
                text = <"circumference">
                description = <"The circumference or perimeter of a 2-dimensional view or slice of the feature.">
            >
            ["id4"] = <
                text = <"volume">
                description = <"The volume of a 3-dimensional feature. For instance, if an irregular cavity, the amount of liquid required to fill the cavity.">
            >
            ["id5"] = <
                text = <"width">
                description = <"The measurement of extent of a feature from side to side, perpendicular to the greatest dimension. ">
            >
            ["id6"] = <
                text = <"depth">
                description = <"The extent downward or inward; the perpendicular measurement from the surface to determine the deepness of a feature.">
            >
            ["id7"] = <
                text = <"length">
                description = <"The length of a 2-dimensional view or slice of a feature.">
            >
